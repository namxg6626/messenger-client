{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"],
      "@utils/*": ["./src/utils/*"],
      "@store/*": ["./src/store/*"],
      "@socket/*": ["./src/socket/*"],
      "@models/*": ["./src/typdef/models/*"],
      "@modules/*": ["./src/modules/*"],
      "@http/*": ["./src/http/*"]
    }
  },
  "include": ["src", "craco.config.js"]
}
